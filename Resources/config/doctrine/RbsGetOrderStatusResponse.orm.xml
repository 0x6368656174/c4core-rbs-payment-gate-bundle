<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                  http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="ItQuasar\RbsPaymentGateBundle\Entity\RbsGetOrderStatusResponse" table="rbs_get_order_status_responses">
        <field name="orderStatus" type="integer" nullable="true"/>
        <field name="errorCode" type="integer" nullable="true"/>
        <field name="errorMessage" type="string" nullable="true"/>
        <field name="orderNumber" type="string"/>
        <field name="pan" type="string" nullable="true"/>
        <field name="expiration" type="string" nullable="true"/>
        <field name="cardholderName" type="string" nullable="true"/>
        <field name="amount" type="integer"/>
        <field name="currency" type="string" nullable="true"/>
        <field name="approvalCode" type="string" nullable="true"/>
        <field name="authCode" type="string" nullable="true"/>
        <field name="ip" type="string" nullable="true"/>
        <field name="date" type="string"/>
        <field name="orderDescription" type="string"/>
        <field name="actionCodeDescription" type="string"/>
    </entity>

</doctrine-mapping>