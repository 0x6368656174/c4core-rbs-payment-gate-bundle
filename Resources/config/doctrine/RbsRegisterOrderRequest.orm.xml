<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                  http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="ItQuasar\C4CoreRbsPaymentGateBundle\Entity\RbsRegisterOrderRequest" table="rbs_register_order_requests">
        <field name="merchantOrderNumber" type="string" />
        <field name="description" type="string" nullable="true" />
        <field name="amount" type="integer" />
        <field name="currency" type="string" nullable="true" />
        <field name="language" type="string" nullable="true" />
        <field name="pageView" type="string" nullable="true" />
        <field name="sessionTimeoutSecs" type="integer" nullable="true" />
        <field name="bindingId" type="integer" nullable="true" />
        <field name="expirationDate" type="string" nullable="true" />
        <field name="returnUrl" type="string" />
        <field name="failUrl" type="string" nullable="true" />
        <field name="params" type="array" nullable="true" />
        <field name="clientId" type="integer" nullable="true" />
        <field name="merchantLogin" type="string" nullable="true" />
    </entity>

</doctrine-mapping>